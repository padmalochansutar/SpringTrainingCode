package com.csmtech.runner;

import com.csmtech.AccessmentString;

public class Runner {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		AccessmentString accessment = new AccessmentString("cde");
				int rest = accessment.Check();
				if(rest==1) {
					System.out.println("Digit Only");
				}
				else if(rest==2){
					System.out.println("Alpha Numeric");
				}
				else if(rest==3) {
					System.out.println("It's String");
				}
			}

		}


